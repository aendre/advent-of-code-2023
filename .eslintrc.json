{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
      "airbnb-base",
      "airbnb-typescript/base",
      "plugin:@typescript-eslint/recommended"
    ],
    "overrides": [
    ],
    "plugins": ["no-autofix"],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
        "project": "./tsconfig.eslint.json"
    },
    "rules": {
      "no-console": "off",
      "max-len": "off",
      "import/prefer-default-export" : "off",
      "semi": "off",
      "@typescript-eslint/semi": "off",
      "arrow-parens": ["error", "as-needed"],
      "@typescript-eslint/no-unused-vars": "off",
      "default-case": "off",
      //https://stackoverflow.com/questions/45443478/eslint-autofix-ignore-rule
      "prefer-const": "off",
      "no-autofix/prefer-const": "warn"
    }
}
